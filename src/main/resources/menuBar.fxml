<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuBarController">
    <children>
        <Button fx:id="save" onAction="#save" text="Zapisz"  />
        <Button fx:id="saveAs" onAction="#saveAs" text="Zapisz jako" HBox.hgrow="ALWAYS" />
        <Button fx:id="undo" onAction="#undo" text="Cofnij" HBox.hgrow="ALWAYS" />
        <Button fx:id="redo" onAction="#redo" text="Ponów" HBox.hgrow="ALWAYS" />
        <Button fx:id="exportResults" onAction="#exportResults" text="Eksportuj wyniki" HBox.hgrow="ALWAYS" />
        <Button fx:id="adminMode" onAction="#adminModeOn" text="Tryb administratora" HBox.hgrow="ALWAYS" />
        <Button fx:id="changePassword" onAction="#changePassword"  text="Zmień hasło" />
    </children>
</HBox>
